<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>GET Users</title>
</head>
<body>
    
    <table border="1">
        <tr>
        <th>User ID</th>
        <th>Name</th>
        <th>Class</th>
        <th>Stream</th>
        <th>Created On</th>
        <th>Action</th>
        </tr>
        <% studentsData.forEach(element => { %>
            <tr>
            <td><%= element.id %></td>
            <td><%= element.name %></td>
            <td><%= element.class %></td>
            <td><%= element.stream %></td>
            <td><%= element.userCreatedDate %></td>
            <td><a href="/deleteuser/<%= element.id %>"><button>DELETE</button></a></td>
            </tr>
        <% }); %>
        </table>

</body>
</html>